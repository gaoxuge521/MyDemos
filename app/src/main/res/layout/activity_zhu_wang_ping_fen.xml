<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:paddingBottom="16dp"
        android:paddingTop="16dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal">

            <FrameLayout
                android:layout_width="0dp"
                android:layout_height="150dp"
                android:layout_weight="1"
                android:clipChildren="false">

                <me.xiaopan.swsv.SpiderWebScoreView
                    android:id="@+id/spiderWeb_mainActivity_1"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:layout_gravity="center"
                    app:angleCount="3"
                    app:hierarchyCount="3" />

                <me.xiaopan.swsv.CircularLayout
                    android:id="@+id/layout_mainActivity_circular1"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:layout_gravity="center"
                    android:clipChildren="false">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="1.0"
                        android:textSize="10dp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="8.0"
                        android:textSize="10dp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="5.0"
                        android:textSize="10dp" />
                </me.xiaopan.swsv.CircularLayout>
            </FrameLayout>

            <FrameLayout
                android:layout_width="0dp"
                android:layout_height="150dp"
                android:layout_weight="1"
                android:clipChildren="false">

                <me.xiaopan.swsv.SpiderWebScoreView
                    android:id="@+id/spiderWeb_mainActivity_2"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:layout_gravity="center" />

                <me.xiaopan.swsv.CircularLayout
                    android:id="@+id/layout_mainActivity_circular2"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:layout_gravity="center"
                    android:clipChildren="false">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="7.0"
                        android:textSize="10dp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="8.0"
                        android:textSize="10dp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="5.0"
                        android:textSize="10dp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="5.0"
                        android:textSize="10dp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="8.0"
                        android:textSize="10dp" />
                </me.xiaopan.swsv.CircularLayout>
            </FrameLayout>
        </LinearLayout>

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="150dp"
            android:layout_weight="1.3"
            android:clipChildren="false">

            <me.xiaopan.swsv.SpiderWebScoreView
                android:id="@+id/spiderWeb_mainActivity_5"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_gravity="center"
                app:angleCount="6"/>

            <me.xiaopan.swsv.CircularLayout
                android:id="@+id/layout_mainActivity_circular5"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_gravity="center"
                android:clipChildren="false">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:drawableRight="@drawable/vip_icon7"
                    android:drawablePadding="4dp"
                    android:gravity="center"
                    android:textSize="10dp"
                    android:text="蓝钻"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:drawableRight="@drawable/vip_icon8"
                    android:drawablePadding="4dp"
                    android:gravity="center"
                    android:textSize="10dp"
                    android:text="红钻"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:drawableRight="@drawable/vip_icon5"
                    android:drawablePadding="4dp"
                    android:gravity="center"
                    android:textSize="10dp"
                    android:text="紫钻"/>

                <TextView
                    android:layout_width="wrap_content"
