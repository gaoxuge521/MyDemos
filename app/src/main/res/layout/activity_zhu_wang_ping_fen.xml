                <me.xiaopan.swsv.CircularLayout
                    android:id="@+id/layout_mainActivity_circular4"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:layout_gravity="center"
                    android:clipChildren="false">

                    <TextView
